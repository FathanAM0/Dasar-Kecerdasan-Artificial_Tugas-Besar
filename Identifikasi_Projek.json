                }
            },
            "fuzzy_rules": [
                "IF pitch IS rendah AND spectral_centroid IS lembut AND rms_energy IS lemah THEN output IS noise",
                "IF pitch IS tinggi AND spectral_centroid IS tajam AND rms_energy IS kuat THEN output IS burung",
                "IF pitch IS sedang AND spectral_centroid IS normal AND rms_energy IS sedang THEN output IS burung"
